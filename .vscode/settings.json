{
    "workbench.iconTheme": "vscode-icons",

    "emmet.includeLanguages": {
        "javascipt": "javascriptreact",
        "vue-html": "html",
        "vue": "javascript",
        "javascript": "javascript",
    },
    "vetur.format.options.tabSize": 2,
    "prettier.tabWidth": 2,
    "[javascript, vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html, vue, vue-html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}